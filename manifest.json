{
	"name": "Classtab.org Dark Mode",
	"version": "0.0.1",
	"description": "A dark mode theme for Classtab.org",
	"permissions": [
		"activeTab",
		"declarativeContent",
		"storage"
	],
	"content_scripts": [
		{
			"matches": ["http://www.classtab.org/*"],
			"css": ["src/css/darcula.css"]
		}
	],
	"web_accessible_resources": ["*.ttf"],
	"background": {
		"scripts": ["src/js/background.js"],
		"persistent": false
	},
	"page_action": {
		"default_icon": {
			"16": "src/img/logo-16.png",
			"32": "src/img/logo-32.png",
			"48": "src/img/logo-48.png",
			"64": "src/img/logo-64.png",
			"128": "src/img/logo-128.png"
		},
		"default_popup": "src/popup.html"
	},
	"icons": {
		"16": "src/img/logo-16.png",
		"32": "src/img/logo-32.png",
		"48": "src/img/logo-48.png",
		"64": "src/img/logo-64.png",
		"128": "src/img/logo-128.png"
	},
	"manifest_version": 2
}